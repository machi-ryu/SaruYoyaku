<h1>Places#edit</h1>
<p>Find me in app/views/places/edit.html.erb</p>

<%= form_for(@place) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class:'form-control' %>

    <%= f.label :adress %>
    <%= f.text_field :adress, class:'form-control' %>

    <%= f.label :detail %>
    <%= f.text_field :detail, class:'form-control' %>

    <%= f.submit "登録", class:"btn btn-primary" %>
<% end %>