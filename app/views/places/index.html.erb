<h1>Places#index</h1>
<p>Find me in app/views/places/index.html.erb</p>

<%= user_signed_in? ? (link_to "施設登録", new_place_path) : nil %>

<% @places.each do |place| %>
    <div class="place">
        <%= link_to place.name, place %>
        <%= place.adress %>
        <%= place.detail %>
    </div>
<% end %>
